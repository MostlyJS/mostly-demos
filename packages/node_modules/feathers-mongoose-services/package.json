{
  "name": "mostly-feathers-mongoose-services",
  "version": "1.0.0",
  "description": "MostlyJS microservice integration demo with feathers-mongoose",
  "author": "Forafish",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run compile",
    "publish": "git push origin --tags && git push origin",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "compile": "rimraf -rf lib/ && babel --source-maps -d lib/ src/",
    "watch": "babel --watch -d lib/ src/",
    "lint": "eslint-if-supported semistandard --fix",
    "mocha": "mocha --opts mocha.opts",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- --opts mocha.opts",
    "test": "npm run compile && npm run lint && npm run coverage"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bluebird": "^3.5.0",
    "feathers-authentication": "^1.2.2",
    "feathers-hooks": "^2.0.0",
    "feathers-memory": "^1.1.0",
    "glob": "^7.1.1",
    "lodash": "^4.17.4",
    "mongoose": "^4.9.8",
    "mongoose-timestamp": "^0.6.0",
    "mostly-entity": "^1.0.6",
    "mostly-feathers": "^1.0.4",
    "mostly-feathers-mongoose": "^1.0.6",
    "mostly-node": "^1.0.6",
    "nats": "^0.7.20"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-root-import": "^5.1.0",
    "babel-preset-env": "^1.4.0",
    "eslint-if-supported": "^1.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "^3.1.0",
    "gulp-eslint": "^3.0.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.6.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.3.0",
    "rimraf": "^2.6.1"
  }
}
