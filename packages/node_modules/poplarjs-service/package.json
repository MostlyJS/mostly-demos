{
  "name": "mostly-poplarjs-service-demo",
  "version": "1.0.0",
  "description": "MostlyJS microservice integration demo with poplarjs",
  "author": "Forafish",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run compile",
    "debug": "node --nolazy --inspect-brk=6904 index.js",
    "publish": "git push origin --tags && git push origin",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "compile": "rimraf -rf lib/ && babel src/ -d lib/ --copy-files",
    "watch": "babel --watch src/ --source-maps inline -d lib/ --copy-files",
    "lint": "eslint-if-supported semistandard --fix",
    "mocha": "mocha --opts mocha.opts",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- --opts mocha.opts",
    "test": "npm run compile && npm run lint && npm run coverage"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "debug": "^2.6.6",
    "mostly-entity": "^1.0.7",
    "mostly-node": "^1.0.7",
    "mostly-poplarjs": "^1.0.8",
    "nats": "^0.7.20"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-root-import": "^5.1.0",
    "babel-preset-env": "^1.4.0",
    "eslint-if-supported": "^1.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "^3.1.0",
    "gulp-eslint": "^3.0.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.6.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.3.0",
    "rimraf": "^2.6.1"
  }
}
